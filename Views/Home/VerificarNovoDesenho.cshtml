<div v-if="formulario.paginas[paginaAtual] && formulario.paginas[paginaAtual].conteudos[conteudoSelecionado]" @*v-if="conteudoEditando != -1"*@ @*v-if="index == conteudoEditando && conteudo.tipo == 11"*@ class="modal fade" id="modalVerificarNovoDesenho" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">{{ formulario.paginas[paginaAtual].conteudos[conteudoSelecionado].mensagemNovoDesenho }}</h5>
                <button type="button" class="close" @*data-dismiss="modal"*@ aria-label="Close" v-on:click="excluirDesenho()">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            @*<div class="modal-body" style="padding-top: unset;">
                
            </div>*@
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" v-on:click="formulario.paginas[paginaAtual].conteudos[conteudoSelecionado].novoDesenho(false)">Não</button>
                <button type="button" class="btn btn-success" v-on:click="formulario.paginas[paginaAtual].conteudos[conteudoSelecionado].novoDesenho(true)">Sim</button>
            </div>
        </div>
    </div>
</div>
